x = [19 22 25 28 32 35];
y = [0.66 0.367 0.223 0.14 0.084 0.06];
A = [x', ones(size(x'))];
n = length(x);
heso = A\y';
goc = heso(1);
chan = heso(2);
yi = goc * x + chan;
ss = y - yi;
tbc = sum(ss.^2) / n;
disp('ham xap xi:');
disp(['y = ', num2str(goc), ' * x + ', num2str(chan)]);
disp(['sai so trung binh: ', num2str(tbc)]);
figure;
plot(x, y, 'o', x, yi, '-');
legend('Du lieu', 'Ham xao xi');
xlabel('x');
ylabel('y');
title('Xap xi binh phuong be nhat');
